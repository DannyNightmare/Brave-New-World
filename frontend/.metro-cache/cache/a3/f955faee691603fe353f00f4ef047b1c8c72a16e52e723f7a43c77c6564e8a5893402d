power_tier || null,\n          power_max_level: newItem.power_max_level || null,\n          next_tier_ability: newItem.next_tier_ability || null\n        };\n        if (newItem.image) {\n          payload.image = newItem.image;\n        }\n        if (Object.keys(stat_boost).length > 0) {\n          payload.stat_boost = stat_boost;\n        }\n        const response = await fetch(`${API_URL}/api/shop`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify(payload)\n        });\n        if (response.ok) {\n          setNewItem({\n            name: '',\n            description: '',\n            price: 50,\n            stock: 1,\n            category: 'general',\n            image: '',\n            is_power: false,\n            power_category: '',\n            power_tier: 'Base',\n            power_max_level: 5,\n            next_tier_ability: '',\n            item_type: 'weapon',\n            strength_boost: 0,\n            intelligence_boost: 0,\n            vitality_boost: 0\n          });\n          setModalVisible(false);\n          fetchShopItems();\n          fetchPowerCategories(); // Refresh power categories\n          Alert.default.alert('Success', 'Item added to shop');\n        }\n      } catch (error) {\n        console.error('Failed to create item:', error);\n        Alert.default.alert('Error', 'Failed to create item');\n      }\n    };\n    const purchaseItem = async item => {\n      if (!user?.id) return;\n      if (user.gold < item.price) {\n        Alert.default.alert('Not Enough Gold', `You need ${item.price} gold but only have ${user.gold}`);\n        return;\n      }\n      try {\n        const response = await fetch(`${API_URL}/api/shop/purchase`, {\n          method: 'POST',\n          headers: {\n            'Content-Type': 'application/json'\n          },\n          body: JSON.stringify({\n            user_id: user.id,\n            item_id: item.id\n   